  <forms name="Publication"
      title="Publication"
      description="Form for scientific publications"
      welcomeForm="true">
    <entity
        href="publicationEntityModel.xmi#//@entityModelElements.0"/>
    <pages title="Publication Details">
      <pageElements
          xsi:type="forms:TextField"
          label="Title"
          elementID="01"
          format="^[a-zA-Z -]+$">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.0"/>
      </pageElements>
      <pageElements
          xsi:type="forms:TextField"
          label="Keywords"
          elementID="02"
          format="^([a-zA-Z]+, )+[a-zA-Z]+$">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.1"/>
      </pageElements>
      <pageElements
          xsi:type="forms:TextArea"
          label="Abstract"
          elementID="03">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.2"/>
      </pageElements>
      <pageElements
          xsi:type="forms:TextField"
          label="DOI"
          elementID="04">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.3"/>
      </pageElements>
      <pageElements
          xsi:type="forms:TextField"
          label="Year"
          elementID="05"
          format="^[0-9]+$">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.4"/>
      </pageElements>
      <pageElements
          xsi:type="forms:TextField"
          label="From page"
          elementID="06"
          format="^[0-9]+$">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.5"/>
      </pageElements>
      <pageElements
          xsi:type="forms:TextField"
          label="Until page"
          elementID="07"
          format="^[0-9]+$">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.6"/>
      </pageElements>
      <pageElements
          xsi:type="forms:SelectionField"
          label="Publication type"
          elementID="08">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.7"/>
      </pageElements>
      <pageElements
          xsi:type="forms:SelectionField"
          label="Assigned faculty"
          elementID="09">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.8"/>
      </pageElements>
      <pageElements
          xsi:type="forms:SelectionField"
          label="Research field"
          elementID="10">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.9"/>
      </pageElements>
    </pages>
    <pages title="Authors">
      <pageElements
          xsi:type="forms:Table"
          label="Authors"
          elementID="11"
          editingForm="//@forms.1">
        <relationship
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.10"/>
        <columns
            label="First name"
            elementID="12">
          <attribute
              href="publicationEntityModel.xmi#//@entityModelElements.1/@features.0"/>
        </columns>
        <columns
            label="Last name"
            elementID="13">
          <attribute
              href="publicationEntityModel.xmi#//@entityModelElements.1/@features.1"/>
        </columns>
        <columns
            label="E-Mail"
            elementID="14">
          <attribute
              href="publicationEntityModel.xmi#//@entityModelElements.1/@features.2"/>
        </columns>
      </pageElements>
    </pages>
    <pages title="Proceedings">
      <pageElements
          xsi:type="forms:List"
          label="Proceedings"
          elementID="15"
          editingForm="//@forms.2">
        <relationship
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.11"/>
      </pageElements>
      <condition
          xsi:type="forms:CompositeCondition"
          type="Show"
          conditionID="1"
          compositionType="Or">
        <composedConditions
            xsi:type="forms:AttributeValueCondition"
            type="Show"
            conditionID="2"
            value="CP">
          <attribute
              href="publicationEntityModel.xmi#//@entityModelElements.0/@features.7"/>
        </composedConditions>
        <composedConditions
            xsi:type="forms:AttributeValueCondition"
            type="Show"
            conditionID="3"
            value="WP">
          <attribute
              href="publicationEntityModel.xmi#//@entityModelElements.0/@features.7"/>
        </composedConditions>
      </condition>
    </pages>
    <pages title="Journal">
      <pageElements
          xsi:type="forms:List"
          label="Journal"
          elementID="16"
          editingForm="//@forms.3">
        <relationship
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.12"/>
      </pageElements>
      <condition
          xsi:type="forms:AttributeValueCondition"
          type="Show"
          conditionID="4"
          value="JA">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.0/@features.7"/>
      </condition>
    </pages>
    <pages title="Book">
      <pageElements
