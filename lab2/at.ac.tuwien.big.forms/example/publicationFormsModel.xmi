      </pageElements>
      <pageElements
          xsi:type="forms:TextField"
          label="E-Mail"
          elementID="20"
          format="^\w+@[a-zA-Z_]+\.[a-zA-Z]{2,3}$">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.1/@features.2"/>
      </pageElements>
      <pageElements
          xsi:type="forms:SelectionField"
          label="Faculty-external"
          elementID="21">
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.1/@features.3"/>
      </pageElements>
      <pageElements
          xsi:type="forms:SelectionField"
          label="Faculty"
          elementID="22">
        <condition
            xsi:type="forms:AttributeValueCondition"
            conditionID="6"
            value="true">
          <attribute
              href="publicationEntityModel.xmi#//@entityModelElements.1/@features.3"/>
        </condition>
        <attribute
            href="publicationEntityModel.xmi#//@entityModelElements.1/@features.4"/>
      </pageElements>
    </pages>
  </forms>
